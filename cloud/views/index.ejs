
<!DOCTYPE html>
<html>
  <head>
    <title>ssslack</title>
    <link rel="stylesheet" href="/style.css" media="screen" title="" charset="utf-8">
  </head>
  <body>
    <% snippet.get('messages').forEach(function(message){ %>
      <div class="message">
        <% if (message.imageUrl) { %>
          <img class="image" src="<%= message.imageUrl %>"/>
        <% } %>

        <div class="right">
          <span class="sender">
            <%= message.sender %>
          </span>

          <span class="content">
            <%= message.content %>
          </span>
        </div>
      </div>
    <% }) %>
  </body>
</html>
